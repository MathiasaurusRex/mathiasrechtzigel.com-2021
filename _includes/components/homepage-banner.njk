<section class="title-banner title-banner--outer">
  <div class="title-banner--inner">
    <h1>
      <small class="section-head">{{ tag }}</small>
      {{ tagline | safe}}
    </h1>
    {%- if bannerCTA %}
      <a class="button" href={{bannerCTA.href}}>{{bannerCTA.text}}</a>
    {%- endif %}
  </div>
</section>
